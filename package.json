{
  "name": "ts-starter",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-guzman/ts-starter.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "coverage": "vitest run --coverage",
    "dev": "tsdown --watch",
    "fmt": "oxfmt --check",
    "fmt:fix": "oxfmt",
    "knip": "knip",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepack": "pnpm build && clean-pkg-json",
    "prepare": "is-in-ci || lefthook install",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "20.4.1",
    "@jimmy.codes/eslint-config": "7.1.0",
    "@types/node": "24.10.7",
    "@vitest/coverage-v8": "4.0.18",
    "clean-pkg-json": "1.3.0",
    "commitlint": "20.4.1",
    "eslint": "9.39.2",
    "gitzy": "6.5.7",
    "is-in-ci": "2.0.0",
    "jiti": "2.6.1",
    "knip": "5.83.1",
    "lefthook": "2.1.0",
    "oxfmt": "0.28.0",
    "publint": "0.3.17",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.1.0",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": "^20.19.0 || ^22.13.0 || >=24"
  },
  "packageManager": "pnpm@10.29.1",
  "gitzy": {
    "useCommitlintConfig": true
  }
}
