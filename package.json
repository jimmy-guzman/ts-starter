{
  "name": "ts-starter",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/jimmy-guzman/ts-starter.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "coverage": "vitest run --coverage",
    "dev": "tsdown --watch",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint --color .",
    "lint:fix": "pnpm lint --fix",
    "prepack": "pnpm build && clean-pkg-json",
    "prepare": "is-ci || lefthook install",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "devDependencies": {
    "@commitlint/config-conventional": "19.8.1",
    "@jimmy.codes/eslint-config": "5.14.0",
    "@jimmy.codes/prettier-config": "1.5.0",
    "@types/node": "22.15.27",
    "@vitest/coverage-v8": "3.1.4",
    "clean-pkg-json": "1.3.0",
    "commitlint": "19.8.1",
    "eslint": "9.27.0",
    "gitzy": "5.8.1",
    "is-ci": "4.1.0",
    "jiti": "2.4.2",
    "knip": "5.59.1",
    "lefthook": "1.11.13",
    "prettier": "3.5.3",
    "tsdown": "0.12.5",
    "typescript": "5.8.3",
    "vitest": "3.1.4"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">= 20"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitzy": {
    "useCommitlintConfig": true
  }
}
